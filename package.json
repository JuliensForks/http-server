{
  "name": "@adonisjs/http-server",
  "version": "1.2.8",
  "description": "Extracted copy of AdonisJs HTTP server along with it's router",
  "files": [
    "build/src",
    "build/providers",
    "build/adonis-typings",
    "build/standalone.d.ts",
    "build/standalone.js"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && typedoc --theme markdown --excludePrivate --HideSources && git add docs",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "keywords": [
    "http",
    "http-server"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/encryption": "^1.0.1",
    "@adonisjs/fold": "^6.1.9",
    "@adonisjs/logger": "^1.1.6",
    "@adonisjs/mrm-preset": "^2.1.0",
    "@adonisjs/profiler": "^1.2.4",
    "@poppinss/dev-utils": "^1.0.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.2",
    "@types/pluralize": "0.0.29",
    "@types/proxy-addr": "^2.0.0",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.0.4",
    "japa": "^3.0.0",
    "mrm": "^1.2.2",
    "np": "^5.0.3",
    "pem": "^1.14.2",
    "reflect-metadata": "^0.1.13",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^2.1.11",
    "typescript": "^3.6.2"
  },
  "peerDependencies": {
    "@adonisjs/encryption": "^1.x.x",
    "@adonisjs/logger": "^1.x.x",
    "@adonisjs/profiler": "^1.x.x"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/cookie": "^1.0.2",
    "@poppinss/utils": "^2.0.0",
    "accepts": "^1.3.7",
    "co-compose": "^5.1.1",
    "content-disposition": "^0.5.3",
    "cuid": "^2.1.6",
    "destroy": "^1.0.4",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "haye": "^2.0.2",
    "lodash": "^4.17.15",
    "macroable": "^2.0.2",
    "matchit": "git+https://github.com/thetutlage/matchit.git",
    "ms": "^2.1.2",
    "on-finished": "^2.3.0",
    "pluralize": "^8.0.0",
    "proxy-addr": "^2.0.5",
    "qs": "^6.8.0",
    "ts-essentials": "^3.0.2",
    "type-is": "^1.6.18",
    "vary": "^1.1.2"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/http-server.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/http-server/issues"
  },
  "homepage": "https://github.com/adonisjs/http-server#readme"
}
